{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "clui": "^0.3.1",
    "figlet": "^1.2.0"
  },
  "jshintConfig": {
    "curly": true,
    "devel": true,
    "eqeqeq": true,
